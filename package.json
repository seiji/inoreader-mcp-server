{
  "name": "inoreader-mcp-server",
  "version": "0.1.0",
  "description": "MCP Server for Inoreader RSS feed management",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "inoreader-mcp": "src/index.ts"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun run --watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src/",
    "format": "biome format --write src/"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@types/bun": "^1.1.0",
    "typescript": "^5.7.0"
  },
  "keywords": [
    "mcp",
    "inoreader",
    "rss",
    "llm",
    "claude"
  ],
  "license": "MIT"
}
